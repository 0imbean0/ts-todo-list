{
  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",
    "typeRoots": ["./@types"],
    "allowJs": true,
    "declaration": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}